// Run docker build
properties([disableConcurrentBuilds()])

pipeline {
    agent any
    parameters {
        string(name: "Branch_name", defaultValue: "staging")
    }    
    
    stages {
        stage("create docker image") {
            steps {
                echo " ============== startuem =================="
                dir ('.') {
                	sh 'docker build -t test .'
                }
            }
        }
    }
}
